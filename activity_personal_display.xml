<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lbluebg"
    tools:context=".PersonalDisplay">

    <TextView
        android:id="@+id/idtv"
        android:layout_width="101dp"
        android:layout_height="46dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/designtv"
        android:layout_marginBottom="48dp"
        android:text="Id :"
        android:textAlignment="textEnd"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/enddtv"
        android:layout_width="101dp"
        android:layout_height="46dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/idtv"
        android:layout_marginBottom="113dp"
        android:text="Skills :"
        android:textAlignment="textEnd"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/startdtv"
        android:layout_width="101dp"
        android:layout_height="46dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/idtv"
        android:layout_marginBottom="177dp"
        android:text="Link :"
        android:textAlignment="textEnd"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/loctv"
        android:layout_width="101dp"
        android:layout_height="46dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/idtv"
        android:layout_marginTop="221dp"
        android:text="Contact :"
        android:textAlignment="textEnd"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/orgtv"
        android:layout_width="101dp"
        android:layout_height="46dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/idtv"
        android:layout_marginTop="159dp"
        android:text="Location :"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/designtv"
        android:layout_width="101dp"
        android:layout_height="46dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/getdesigtv"
        android:layout_marginStart="60dp"
        android:text="Name :"
        android:textAlignment="textEnd"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/getidtv"
        android:layout_width="158dp"
        android:layout_height="46dp"
        android:layout_alignStart="@+id/getdesigtv"
        android:layout_alignTop="@+id/idtv"
        android:textAlignment="viewStart"
        android:textColor="#132286"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/getskillstv"
        android:layout_width="158dp"
        android:layout_height="46dp"
        android:layout_alignStart="@+id/getdesigtv"
        android:layout_alignTop="@+id/enddtv"
        android:textAlignment="viewStart"
        android:textColor="#132286"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/getlinktv"
        android:layout_width="158dp"
        android:layout_height="46dp"
        android:layout_alignStart="@+id/getdesigtv"
        android:layout_alignTop="@+id/startdtv"
        android:textAlignment="viewStart"
        android:textColor="#132286"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/getcontacttv"
        android:layout_width="158dp"
        android:layout_height="46dp"
        android:layout_alignStart="@+id/getdesigtv"
        android:layout_alignTop="@+id/loctv"
        android:textAlignment="viewStart"
        android:textColor="#132286"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/getloctv"
        android:layout_width="158dp"
        android:layout_height="46dp"
        android:layout_alignStart="@+id/getdesigtv"
        android:layout_alignTop="@+id/orgtv"
        android:textAlignment="viewStart"
        android:textColor="#132286"
        android:textSize="23sp" />

    <TextView
        android:id="@+id/getdesigtv"
        android:layout_width="158dp"
        android:layout_height="46dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="92dp"
        android:textAlignment="viewStart"
        android:textColor="#132286"
        android:textSize="23sp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="100dp"
        android:layout_height="54dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp"
        app:srcCompat="@drawable/usersignin" />
</RelativeLayout>